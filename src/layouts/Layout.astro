---
interface Props {
  title?: string;
}

import '../reset.css';

const { title = 'Feed thing' } = Astro.props;
---

<!doctype html>
<html lang="en">
  <head>
    <title>{title}</title>
    <meta charset="utf-8" />
    <meta
      name="viewport"
      content="width=device-width, initial-scale=1.0, viewport-fit=cover"
    />
    <meta
      name="theme-color"
      content="#000000"
      media="(prefers-color-scheme: dark)"
    />
    <meta name="theme-color" content="#FFFFFF" />
  </head>
  <body><slot /></body>
</html>
